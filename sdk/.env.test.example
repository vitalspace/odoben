# SDK Test Configuration
# Copy this file to .env.test and fill in your test values

# API Key for authentication (get from backend /api/v1/apikey/generate)
TEST_API_KEY=your_api_key_here

# Backend URL (use https://odoben.xyz/api/v1 for production, or http://odoben.xyz/api/v1 if HTTPS doesn't work)
TEST_BASE_URL=https://odoben.xyz/api/v1

# Test user wallet address (should match the API key owner)
TEST_USER_ADDRESS=your_wallet_address_here
